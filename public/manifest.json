{
  "manifest_version": 2,
  "short_name": "Basic Websocket Client",
  "name": "Basic Websocket Client",
  "description": "A basic websocket client to send and receive messages",
  "version": "1.0",
  "icons": {
    "32": "favicon_32.png"
  },
  "browser_action": {
    "default_icon": "favicon_32.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["launcher.js"]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
